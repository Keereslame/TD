         </div>\n            </b-col>\n        </b-row>\n    </b-container>\n</template>\n\n<script>\n    import Navbar from \"../components/Navbar\";\n    import Sidebar from \"../components/Sidebar\";\n    import Influx from \"influx\";\n\n    export default {\n        name: \"Measure\",\n        components: {\n            Sidebar,\n            Navbar,\n        },\n        data: function() {\n        return {\n            client: new Influx.InfluxDB({\n                host: 'localhost',\n                port: 8086,\n                database: 'ttn_lowimpact_food',\n                schema: [\n                    {\n                        measurement: 'ttn_lht65',\n                        fields: {\n                            BatV: Influx.FieldType.FLOAT,\n                            Hum_SHT: Influx.FieldType.FLOAT,\n                            TempC_DS: Influx.FieldType.FLOAT,\n                            TempC_SHT: Influx.FieldType.FLOAT,\n                            time: Influx.FieldType.INTEGER\n                        },\n                        tags: [\n                        ]\n                    }\n                ]\n            }),\n            temperture_supp: 0,\n            Measurements: []\n        }\n    },\n    created: function() {\n        console.log(this.client);\n    },\n    mounted() {\n        this.client.query('select TempC_SHT from ttn_lht65 ').then(results => {\n            this.temperture_supp = results\n        })\n    }\n    }\n</script>\n\n<style scoped>\n    .col-auto{\n        background-color: #95ca91;\n        padding-right: 0px;\n    }\n    .card-header{\n        background-color: lightcoral;\n        text-align: left;\n    }\n    .container-fluid{\n        padding-right: 0px;\n        padding-left: 0px;\n    }\n    .nav-link{\n        color: #2c3e50;\n    }\n\n</style>"]}]}
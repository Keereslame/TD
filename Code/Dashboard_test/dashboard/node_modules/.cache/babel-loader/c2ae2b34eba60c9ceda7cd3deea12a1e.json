 'ttn_lowimpact_food',\n                    schema: [\n                        {\n                            measurement: 'ttn_lht65',\n                            fields: {\n                                BatV: Influx.FieldType.FLOAT,\n                                Hum_SHT: Influx.FieldType.FLOAT,\n                                TempC_DS: Influx.FieldType.FLOAT,\n                                TempC_SHT: Influx.FieldType.FLOAT,\n                                time: Influx.FieldType.INTEGER\n                            },\n                            tags: [\n                            ]\n                        }\n                    ]\n                }),\n                temperature_high: [],\n                humidity: [],\n                temperature_down: [],\n                time: []\n\n            }\n        },\n        created: function() {\n            this.client.query('select TempC_SHT from ttn_lht65').then(results => {\n                this.temperature_high = results;\n                console.log(\"temperature high:\" + this.temperature_high);\n            })\n\n            this.client.query('select Hum_SHT from ttn_lht65').then(results => {\n                this.humidity = results;\n                console.log(\"humidity:\" + results);\n            })\n\n            this.client.query('select TempC_DS from ttn_lht65').then(results => {\n                this.temperature_down = results;\n                console.log(\"temperature down:\" + results);\n            })\n        }\n    }\n\n</script>\n\n<style scoped>\n    .echarts{\n        max-width: 100%;\n        height: 400px;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}